<% if ( rol=="dueno" ) { %>
    <%# Crear Paseo dueño %>
    <h1 class="text-uppercase text-titulo text-center">CREAR PASEO</h1>
    <p class="fs-4 text-parrafo text-center">
        Adjunta la informacion de tu paseo para poder publicarla.
    </p>
    <form action="/v1/dueno/CrearPaseo" method="post">
        <div class="container ContainerAñadirPerro">
            <div class="row">
                <div class="col-lg-6">
                    <div class="form-group">
                        <%# Datos del mapa %>
                            <label class="TextoMapa">Selecciona el lugar de paseo en el mapa:</label>
                            <%# Mapa %>
                        <div id="map" class="mapaPaseo" alt="Ubicación de dueño"></div>
                            <%# Mapa: latitud %>
                        <input type="hidden" id="paseoLatitude" name="paseoLatitude" required>
                            <%# Mapa: Longitud %>
                        <input type="hidden" id="paseoLongitude" name="paseoLongitude" required>

                        <%# Datos del dueño %>
                        <input type="hidden" id="duenoEmailPaseo" name="duenoEmailPaseo" value="<%= email %>" required>
                        <input type="hidden" id="duenoFotoPaseo" name="duenoFotoPaseo" value="<%= foto %>" required>
                        <input type="hidden" id="duenoNombrePaseo" name="duenoNombrePaseo" value="<%= nombre %>" required>

                                            <%# Datos del paseador %>
                                                <!--<input type="hidden" id="paseadorIdPaseo" name="paseadorIdPaseo" value="<%= email %>">
                        <input type="hidden" id="paseadorImgPaseo" name="paseadorImgPaseo" value="<%= foto %>">
                        <input type="hidden" id="paseadorNombrePaseo" name="paseadorNombrePaseo" value="<%= nombre %>">-->

                                                <%# Datos del paseador %>
                                                    <!--<input type="hidden" id="perroImgPaseo" name="perroImgPaseo" value="lilajpg">
                        <input type="hidden" id="perroLocalizacionPaseo" name="perroLocalizacionPaseo" value="0x0">
                        <input type="hidden" id="perroNombrePaseo" name="perroNombrePaseo" value="lila">-->

                                                    <!--  <input type="hidden" id=""> -->
                                                    <div id="map"></div>
                    </div>
                    <div class="form-group">

                    <% usuario.perros.forEach(function(element) { %>
                        <label>
                            <input type="checkbox" name="perros" value="<%= JSON.stringify(element) %>">
                            <%= element.nombre %>
                        </label>
                        <br>
                    <% }); %>
                        
                </div>
                <div class="form-group ">
                    <label class="TextoMapa">Selecciona una hora de inicio</label>
                    <input class="InputsAñadirPerro" type="date" id="fechaPaseo " name="fechaPaseo"
                        class="form-control " placeholder="Fecha" required>
                    <label class="TextoMapa">Selecciona una hora de inicio</label>
                    <input class="InputsAñadirPerro" type="time" id="horaInicioPaseo" name="horaInicioPaseo"
                        class="form-control " placeholder="Hora inicio" required>
                    <label class="TextoMapa">Selecciona una hora de finalizacion</label>
                    <input class="InputsAñadirPerro" type="time" id="horaFinPaseo" name="horaFinPaseo"
                        class="form-control " placeholder="Hora fin" required>
                </div>

                </div> 
                    <div class="col-lg-6">
                        <div class="form-group">
                            <input class="InputsAñadirPerro" type="text" id="tituloPaseo" name="tituloPaseo"
                                class="form-control" placeholder="Titulo" required>
                        </div>
                        <div class="form-group">
                            <input class="InputsAñadirPerro" type="text" id="descripcionPaseo" name="descripcionPaseo"
                                class="form-control" placeholder="Descripción" required>
                        </div>
                        <!-- INPUT NOMBRE DEL DESTINO -->
                        <div class="form-group">
                            <input class="InputsAñadirPerro" type="text" id="nombreDestinoPaseo"
                                name="nombreDestinoPaseo" class="form-control" placeholder="Lugar del paseo" required>
                        </div> 
                        <div class="form-group">
                            <input class="InputsAñadirPerro" type="text" id="precioPaseo" name="precioPaseo"
                                class="form-control" placeholder="Precio por perro" required>
                        </div>
                        <!-- INPUT METODO DE PAGO -->
                        <div class="form-group">
                            <input class="InputsAñadirPerro" type="text" id="medioPagoPaseo" name="medioPagoPaseo"
                                class="form-control" placeholder="Metodo para realizar el pago" required>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row ">
                <div class="col-lg-12">
                    <div class="form-group a-center">
                        <a href="/MisPerros">
                            <button class="BotonCancelarPerro">Cancelar</button>
                        </a>
                        <input type="submit" class="BotonSubirPerro"></input>
                    </div>
                </div>
            </div>
        </div>
    </form>
<% } else { %>
    <%# Crear paseo paseador %>
    <h1 class="text-uppercase text-titulo-paseador text-center">CREAR PASEO</h1>
    <p class="fs-4 text-parrafo-paseador text-center">
        Adjunta la informacion de tu paseo para poder publicarla.
    </p>
    <form action="">
        <div class="container ContainerAñadirPerro">
            <div class="row">
                <div class="col-lg-6">
                    <div class="form-group">
                        <label class="TextoMapa">Selecciona el lugar de paseo en el mapa:</label>
                        <div id="map"></div>
                    </div>
                    <div class="form-group">
                        <textarea class="InputDescripcionPaseo" name="Descripcion" id="Descripcion"
                            cols="30" rows="10" placeholder="Descripcion"></textarea>
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="form-group">
                        <input class="InputsAñadirPerro" type="text" id="Nombre" name="Nombre"
                            class="form-control" placeholder="Nombre">
                    </div>
                    <div class="form-group ">
                        <input class="InputsAñadirPerro" type="date" id="Fecha " name="Fecha"
                            class="form-control " placeholder="Fecha">
                    </div>
                    <div class="form-group ">
                        <input class="InputsAñadirPerro" type="time" id="Hora " name="Hora "
                            class="form-control " placeholder="Hora">
                    </div>
                    <div class="form-group">
                        <input class="InputsAñadirPerro" type="text" id="Precio" name="Precio"
                            class="form-control" placeholder="Precio por perro">
                    </div>
                    <div class="form-group">
                        <input class="InputsAñadirPerro" type="text" id="MetodoPago" name="MetodoPago"
                            class="form-control" placeholder="Metodo para realizar el pago">
                    </div>
                    <div class="form-group">
                        <label class="TextoImagen">¿Desea repetir el paseo?</label>
                        <label class="TextoImagen">Todos los dias</label>
                        <input class="Checksboxes" type="checkbox" id="CheckboxPaseo" name="CheckboxPaseo"
                            class="form-control">
                        <label class="TextoImagen">De lunes a viernes</label>
                        <input class="Checksboxes" type="checkbox" id="CheckboxPaseo2" name="CheckboxPaseo2"
                            class="form-control">
                        <label class="TextoImagen">Solo por hoy</label>
                        <input class="Checksboxes" type="checkbox" id="CheckboxPaseo3" name="CheckboxPaseo3"
                            class="form-control">
                    </div>
                </div>
            </div>
            <div class="row ">
                <div class="col-lg-12">
                    <div class="form-group a-center">
                        <a href="/MisPerros">
                            <button class="BotonCancelarPerro">Cancelar</button>
                        </a>
                        <input type="submit" class="BotonSubirPerro">Subir</input>
                    </div>
                </div>
            </div>
        </div>
    </form>
<% } %>