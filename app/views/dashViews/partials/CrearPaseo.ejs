<% if ( rol=="dueno" ) { %>
    <%# Crear Paseo dueño %>
        <h1 class="text-uppercase text-titulo text-center">CREAR PASEO</h1>
        <p class="fs-4 text-parrafo text-center">
            Adjunta la informacion de tu paseo para poder publicarla.
        </p>
        <form action="http://localhost:3001/api/paseo/" method="post">
            <div class="container ContainerAñadirPerro">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="form-group">
                            <!-- INPUT LONGITUD Y LATITUD -->
                            <label class="TextoMapa">Selecciona el lugar de paseo en el mapa:</label>
                            <%# Leaflet %>
                                <script src="/v1/js/leafletElegir.js"></script>

                                <!--Ubicación (Latitud)-->
                                <input type="hidden" id="paseoLatitude" name="paseoLatitude">
                                <!--Ubicación (Longitud)-->
                                <input type="hidden" id="paseoLongitude" name="paseoLongitude">
                        </div>

                        <!-- INPUT HIDDEN ID DEL DUEÑO -->
                        <input type="hidden" id="duenoIdPaseo" name="duenoIdPaseo" value="<%= usuario.id %>">
                        <!-- INPUT HIDDEN IMG DEL DUEÑO -->
                        <input type="hidden" id="duenoImgPaseo" name="duenoImgPaseo" value="<%= foto %>">
                        <!-- INPUT HIDDEN NOMBRE DEL DUEÑO -->
                        <input type="hidden" id="duenoNombrePaseo" name="duenoNombrePaseo" value="<%= nombre %>">

                        <!-- INPUT HIDDEN ID DEL PASEADOR -->
                        <input type="hidden" id="paseadorIdPaseo" name="paseadorIdPaseo" value="0">
                        <!-- INPUT HIDDEN IMG DEL PASEADOR -->
                        <input type="hidden" id="paseadorImgPaseo" name="paseadorImgPaseo" value="0">
                        <!-- INPUT HIDDEN NOMBRE DEL PASEADOR -->
                        <input type="hidden" id="paseadorNombrePaseo" name="paseadorNombrePaseo" value="0">

                        <!-- INPUT HIDDEN ID DEL PERRO -->
                        <input type="hidden" id="perroIdPaseo" name="perroIdPaseo" value="0">
                        <!-- INPUT HIDDEN IMG DEL PERRO -->
                        <input type="hidden" id="perroImgPaseo" name="perroImgPaseo" value="0">
                        <!-- INPUT HIDDEN LOCALIZACION DEL PERRO -->
                        <input type="hidden" id="perroLocalizacionPaseo" name="perroLocalizacionPaseo" value="0x0">
                        <!-- INPUT HIDDEN NOMBRE DEL PERRO -->
                        <input type="hidden" id="perroNombrePaseo" name="perroNombrePaseo" value="0">


                        <!-- INPUT SELECCION DE PERROS DEL USUARIO -->
                        <div class="form-group">
                            <textarea class="InputDescripcionPaseo" name="descripcionPaseo" id="descripcionPaseo"
                                cols="30" rows="10" placeholder="Descripcion"></textarea>
                        </div>
                        <!--        <div class="form-group">
                            <label class="TextoMapa">Selecciona los perros para el paseo</label>
                            <br>
                            <label for="perro1">
                                <input type="checkbox" id="perro1" name="perro" value ="perro" %> Perro 1
                            </label>
                            <br>
                            <label for="perro2">
                                <input type="checkbox" id="perro2" name="perro" value ="perro" %> Perro 2
                            </label>
                            <br>
                            <label for="perro3">
                                <input type="checkbox" id="perro3" name="perro" valuee ="perro"%> Perro 3
                            </label>
                            <br>
                            <label for="perro4">
                                <input type="checkbox" id="perro4" name="perro" value ="perro" %> Perro 4
                            </label>
                        </div>
                    </div> -->
                        <!-- INPUT TITULO -->
                        <div class="col-lg-6">
                            <div class="form-group">
                                <input class="InputsAñadirPerro" type="text" id="TituloPaseo" name="TituloPaseo"
                                    class="form-control" placeholder="Titulo">
                            </div>
                            <!-- INPUT FECHA -->
                            <div class="form-group ">
                                <input class="InputsAñadirPerro" type="text" id="fechaPaseo " name="fechaPaseo"
                                    class="form-control " placeholder="Fecha">
                            </div>
                            <!-- INPUT HORA DE INICIO -->
                            <div class="form-group ">
                                <label class="TextoMapa">Selecciona una hora de inicio</label>
                                <input class="InputsAñadirPerro" type="text" id="horaInicioPaseo" name="horaInicioPaseo"
                                    class="form-control " placeholder="Hora inicio">
                            </div>
                            <!-- INPUT HORA FIN -->
                            <div class="form-group ">
                                <label class="TextoMapa">Selecciona una hora de finalizacion</label>
                                <input class="InputsAñadirPerro" type="text" id="horaFinPaseo" name="horaFinPaseo"
                                    class="form-control " placeholder="Hora fin">
                            </div>
                            <!-- INPUT NOMBRE DEL DESTINO -->
                            <div class="form-group">
                                <input class="InputsAñadirPerro" type="text" id="nombreDestinoPaseo"
                                    name="nombreDestinoPaseo" class="form-control" placeholder="Lugar del paseo">
                            </div>
                            <!-- INPUT PRECIO POR PERRO -->
                            <div class="form-group">
                                <input class="InputsAñadirPerro" type="text" id="precioPaseo" name="precioPaseo"
                                    class="form-control" placeholder="Precio por perro">
                            </div>
                            <!-- INPUT METODO DE PAGO -->
                            <div class="form-group">
                                <input class="InputsAñadirPerro" type="text" id="medioPagoPaseo" name="medioPagoPaseo"
                                    class="form-control" placeholder="Metodo para realizar el pago">
                            </div>
                        </div>
                    </div>
                    <div class="row ">
                        <div class="col-lg-12">
                            <div class="form-group a-center">
                                <a href="/MisPerros">
                                    <button class="BotonCancelarPerro">Cancelar</button>
                                </a>
                                <input type="submit" class="BotonSubirPerro">Subir</input>
                            </div>
                        </div>
                    </div>
                </div>
        </form>
        <% } else { %>
            <%# Crear paseo paseador %>
                <h1 class="text-uppercase text-titulo-paseador text-center">CREAR PASEO</h1>
                <p class="fs-4 text-parrafo-paseador text-center">
                    Adjunta la informacion de tu paseo para poder publicarla.
                </p>
                <form action="">
                    <div class="container ContainerAñadirPerro">
                        <div class="row">
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label class="TextoMapa">Selecciona el lugar de paseo en el mapa:</label>
                                    <div id="map"></div>
                                </div>
                                <div class="form-group">
                                    <textarea class="InputDescripcionPaseo" name="Descripcion" id="Descripcion"
                                        cols="30" rows="10" placeholder="Descripcion"></textarea>
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <input class="InputsAñadirPerro" type="text" id="Nombre" name="Nombre"
                                        class="form-control" placeholder="Nombre">
                                </div>
                                <div class="form-group ">
                                    <input class="InputsAñadirPerro" type="date" id="Fecha " name="Fecha"
                                        class="form-control " placeholder="Fecha">
                                </div>
                                <div class="form-group ">
                                    <input class="InputsAñadirPerro" type="time" id="Hora " name="Hora "
                                        class="form-control " placeholder="Hora">
                                </div>
                                <div class="form-group">
                                    <input class="InputsAñadirPerro" type="text" id="Precio" name="Precio"
                                        class="form-control" placeholder="Precio por perro">
                                </div>
                                <div class="form-group">
                                    <input class="InputsAñadirPerro" type="text" id="MetodoPago" name="MetodoPago"
                                        class="form-control" placeholder="Metodo para realizar el pago">
                                </div>
                                <div class="form-group">
                                    <label class="TextoImagen">¿Desea repetir el paseo?</label>
                                    <label class="TextoImagen">Todos los dias</label>
                                    <input class="Checksboxes" type="checkbox" id="CheckboxPaseo" name="CheckboxPaseo"
                                        class="form-control">
                                    <label class="TextoImagen">De lunes a viernes</label>
                                    <input class="Checksboxes" type="checkbox" id="CheckboxPaseo2" name="CheckboxPaseo2"
                                        class="form-control">
                                    <label class="TextoImagen">Solo por hoy</label>
                                    <input class="Checksboxes" type="checkbox" id="CheckboxPaseo3" name="CheckboxPaseo3"
                                        class="form-control">
                                </div>
                            </div>
                        </div>
                        <div class="row ">
                            <div class="col-lg-12">
                                <div class="form-group a-center">
                                    <a href="/MisPerros">
                                        <button class="BotonCancelarPerro">Cancelar</button>
                                    </a>
                                    <input type="submit" class="BotonSubirPerro">Subir</input>
                                </div>
                            </div>
                        </div>
                    </div>
                </form>
                <% } %>